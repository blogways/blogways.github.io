import springfox.documentation.swagger2.annotations.EnableSwagger2;

import java.util.ArrayList;
import java.util.List;

/**
 * Created by liuyw on 16/9/7.
 */
@RestController
@RequestMapping("/user")
@Api("userControllerç›¸å…³api")
@EnableSwagger2
public class UserController {


    @ApiOperation("è·å–ç”¨æˆ·ä¿¡æ¯")
    @ApiImplicitParams({
            @ApiImplicitParam(paramType = "header", name = "username", dataType = "String", required = true, value = "ç”¨æˆ·çš„å§“å", defaultValue = "zhaojigang"),
            @ApiImplicitParam(paramType = "query", name = "password", dataType = "String", required = true, value = "ç”¨æˆ·çš„å¯†ç ", defaultValue = "wangna")
    })
    @ApiResponses({
            @ApiResponse(code = 400, message = "è¯·æ±‚å‚æ•°æ²¡å¡«å¥½"),
            @ApiResponse(code = 404, message = "è¯·æ±‚è·¯å¾„æ²¡æœ‰æˆ–é¡µé¢è·³è½¬è·¯å¾„ä¸å¯¹")
    })
    @RequestMapping(value = "/getUser", method = RequestMethod.GET)
    public List&lt;UserVo&gt; getUser(@RequestHeader("username") String username, @RequestParam("password") String password) {
//        return userService.getUser(username,password);
        UserVo user1 = new UserVo("test01", "æµ‹è¯•è´¦æˆ·ä¸€", "çœŸä»™");
        UserVo user2 = new UserVo("test02", "æµ‹è¯•è´¦æˆ·äºŒ", "ä»™ç‹");
        UserVo user3 = new UserVo("test03", "æµ‹è¯•è´¦æˆ·ä¸‰", "ä»™å¸");
